  "scripts": {
    "dev": "npm-run-all --parallel watch:server watch:prisma watch:schema",
    "watch:server": "nodemon -x",
    "watch:prisma": "nodemon -e prisma -w src/graphql/schema/prisma/typeDefs/datamodel.prisma -x \"prisma generate\"",
    "watch:schema": "nodemon -e graphql -w __generated__/prisma-client/schema.graphql -x \"prisma deploy\"",
    "watch:types": "ttsc -w --project tsconfig.build.json --emitDeclarationOnly --declaration true --declarationMap true",
    "watch:scripts": "webpack --watch",
    "build": "npm-run-all build:prisma generate:graphql",
    "build:types": "ttsc --project tsconfig.build.json --emitDeclarationOnly --declaration true --declarationMap true",
    "build:scripts": "webpack",
    "build:prisma": "prisma generate",
    "lint": "yarn tslint && yarn prettier",
    "prettier": "prettier --config ../../.prettierrc.yml \"**/!(types)/**/*.{ts,tsx}\" --ignore-path ../../.prettierignore --write",
    "tslint": "tslint --project tsconfig.json \"?(src|stories|tests)/**/*.{ts,tsx}\" --fix",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "docker": "docker-compose up -d",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "download:schema": "apollo schema:download --endpoint=http://localhost:4000/graphql schema.json"
  },
  "dependencies": {
    "apollo-fetch": "^0.7.0",
    "apollo-server": "^2.5.0",
